require(questionr)
require(dplyr)
# Tablas por sexo
muj <- ant %>%
  as_tibble() %>%
  filter(SEXO==2)
hom <- ant %>%
  as_tibble() %>%
  filter(SEXO==1)
# Sexo
sex <- ant %>%
  as_tibble() %>%
  mutate(s=case_when(SEXO==1 ~1,
                     SEXO==2 ~2))
table(sex$s)
wtd.table(sex$s,weights = sex$FACTOR)
